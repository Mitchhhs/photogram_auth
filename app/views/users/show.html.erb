<div class="page-header">
  <h3>User <%= @user.username %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Caption</dt>
      <% @photos.each do |photo| %>
      <dd><%= @photo.caption %></dd>

      <dt>Image</dt>
      <dd><%= @photo.image %></dd>

      <dt>User</dt>
      <dd><%= @photo.user_id %></dd>

      <% end %>

    </dl>

  </div>
</div>
